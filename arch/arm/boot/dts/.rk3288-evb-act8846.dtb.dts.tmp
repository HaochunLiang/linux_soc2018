 power-supply = <&vcc_lcd>;
};

&pinctrl {
 lcd {
  lcd_en: lcd-en {
   rockchip,pins = <7 3 0 &pcfg_pull_none>;
  };
 };

 wifi {
  wifi_pwr: wifi-pwr {
   rockchip,pins = <7 9 0 &pcfg_pull_none>;
  };
 };
};
