 sd-uhs-sdr50;
 sd-uhs-sdr104;
 status = "okay";
};

&sdhi1 {
 pinctrl-0 = <&sdhi1_pins>;
 pinctrl-1 = <&sdhi1_pins_uhs>;
 pinctrl-names = "default", "state_uhs";

 vmmc-supply = <&vcc_sdhi1>;
 vqmmc-supply = <&vccq_sdhi1>;
 cd-gpios = <&gpio6 14 1>;
 wp-gpios = <&gpio6 15 0>;
 sd-uhs-sdr50;
 status = "okay";
};

&sdhi2 {
 pinctrl-0 = <&sdhi2_pins>;
 pinctrl-1 = <&sdhi2_pins_uhs>;
 pinctrl-names = "default", "state_uhs";

 vmmc-supply = <&vcc_sdhi2>;
 vqmmc-supply = <&vccq_sdhi2>;
 cd-gpios = <&gpio6 22 1>;
 sd-uhs-sdr50;
 status = "okay";
};

&qspi {
 pinctrl-0 = <&qspi_pins>;
 pinctrl-names = "default";

 status = "okay";

 flash: flash@0 {
  compatible = "spansion,s25fl512s", "jedec,spi-nor";
  reg = <0>;
  spi-max-frequency = <30000000>;
  spi-tx-bus-width = <4>;
  spi-rx-bus-width = <4>;
  spi-cpha;
  spi-cpol;
  m25p,fast-read;

  partitions {
   compatible = "fixed-partitions";
   #address-cells = <1>;
   #size-cells = <1>;

   partition@0 {
    label = "loader";
    reg = <0x00000000 0x00080000>;
    read-only;
   };
   partition@80000 {
    label = "user";
    reg = <0x00080000 0x00580000>;
    read-only;
   };
   partition@600000 {
    label = "flash";
    reg = <0x00600000 0x03a00000>;
   };
  };
 };
};

&msiof0 {
 pinctrl-0 = <&msiof0_pins>;
 pinctrl-names = "default";

 status = "okay";

 pmic: pmic@0 {
